{
    "name": "corona",
    "version": "0.1.0",
    "private": false,
    "dependencies": {
        "@babel/core": "^7.7.7",
        "@emotion/core": "^10.0.35",
        "@react-google-maps/api": "^1.10.1",
        "@types/googlemaps": "^3.39.13",
        "@types/markerclustererplus": "^2.1.33",
        "@types/node": "^13.13.21",
        "@types/react": "^16.9.50",
        "axios": "^0.19.2",
        "core-js": "^3.6.5",
        "eslint-plugin-flowtype": "^3.13.0",
        "eslint-plugin-import": "^2.22.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.0",
        "npm": "^6.14.8",
        "object-hash": "^2.0.3",
        "react": "16.13.0",
        "react-dom": "16.13.0",
        "react-google-maps": "^9.4.5",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^3.4.3",
        "react-twitter-embed": "^3.0.3",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.2",
        "typescript": "^3.9.7"
    },
    "scripts": {
        "hot": "PORT=9091 react-scripts start",
        "build": "CI=true GENERATE_SOURCEMAP=false react-scripts build",
        "dev": "npm run build && serve build/ -l 80",
        "serve": "PORT=$(grep PORT .env | cut -d '=' -f2) && serve -s build -l $PORT",
        "production": "react-scripts build && PORT=$(grep PORT .env | cut -d '=' -f2) && HTTPS=true && SSL_CRT_FILE=/var/www/certificates/cert.pem && SSL_KEY_FILE=/var/www/certificates/privkey.pem && serve --ssl-cert $SSL_CRT_FILE --ssl-key $SSL_KEY_FILE build -l $PORT",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
